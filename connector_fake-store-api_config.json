{
  "name": "fake-store-api",
  "config": {
    "connector.class": "com.tm.kafka.connect.rest.RestSourceConnector",
    "tasks.max": "1",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.storage.StringConverter",
    "rest.source.poll.interval.ms": "60000",
    "rest.source.method": "GET",
    "rest.source.url": "http://fakestoreapi.com/products/1",
    "rest.source.topic.selector": "com.tm.kafka.connect.rest.selector.SimpleTopicSelector",
    "rest.source.headers": "Content-Type:application/json,Accept:application/json",
    "rest.source.destination.topics": "fake-store-api"
  }
}